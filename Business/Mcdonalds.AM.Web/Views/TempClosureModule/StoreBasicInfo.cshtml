@{
    Layout = null;
}
<div class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">
            [[[餐厅基本信息]]]
        </span>
        <div class="panel-tools">
            <a href="{{store.linkUrl}}" class="btn btn-sm btn-default">[[[更多]]] ></a>
        </div>
    </div>
    <div class="panel-body form-horizontal" role="form">
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[餐厅编号]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.StoreCode}}
            </div>
            <label class="col-xs-2 control-label">[[[中文名称]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.NameZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[英文名称]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.NameENUS}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[区域]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.RegionZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[市场]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.MarketZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[城市]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.CityZHCN}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[地址]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.AddressZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[开业日期]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.OpenDate | date:"yyyy-MM-dd"}}
            </div>
            <label class="col-xs-2 control-label">[[[关店日期]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreBasicInfo.CloseDate | date:"yyyy-MM-dd"}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[资产经理]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreDevelop.AssetMgrName}}
            </div>
            <label class="col-xs-2 control-label">[[[资产专员]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreDevelop.AssetRepName}}
            </div>
            <label class="col-xs-2 control-label">[[[资产发起人]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{project.AssetActorNameENUS}}
            </div>
        </div>
    </div>
</div>


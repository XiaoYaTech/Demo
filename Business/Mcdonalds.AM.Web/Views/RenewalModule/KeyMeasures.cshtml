
@{
    Layout = null;
}
<div class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">
            [[[Key Measures]]]
        </span>
    </div>
    <div class="panel-body form-horizontal">
        <div class="form-group">
            <label class="col-xs-3 control-label">
                [[[Fair Market Rent (RMB) per appraisal]]]
            </label>
            <div class="col-xs-3">
                <input type="text" class="form-control" ng-model="analysis.FairMarketRentAmount" number-show="money" disabled="disabled">
            </div>
            <label class="col-xs-3 control-label">
                [[[Lease Tenure & Term]]]
            </label>
            <div class="col-xs-3">
                <input type="text" class="form-control" ng-model="analysis.LeaseTenureAndTerm" disabled="disabled">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label">
                [[[Due Rent for 1st Tenancy Year (New) (RMB)]]]
            </label>
            <div class="col-xs-3">
                <input type="text" class="form-control" ng-model="analysis.DR1stTYAmount" number-show="money" disabled="disabled">
            </div>
            <label class="col-xs-3 control-label">
                [[[Free Rental Period]]]
            </label>
            <div class="col-xs-3">
                <input type="text" class="form-control" ng-model="analysis.FreeRentalPeriod" disabled="disabled">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-3 control-label">
                [[[Rental Deviation%]]]
            </label>
            <div class="col-xs-3">
                <input type="text" class="form-control" ng-model="analysis.RentDeviation" input-percent disabled="disabled">
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-12 control-label">
                [[[Rent Structure (Whole lease term)]]]
            </label>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <pre>{{analysis.RentStructure}}</pre>
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-12 control-label">
                [[[Management Fee Structure]]]
            </label>
        </div>
        <div class="form-group">
            <div class="col-xs-12">
                <pre>{{analysis.MFStructureNew}}</pre>
            </div>
        </div>
        <table class="table table-bordered">
            <tr>
                <th width="25%">[[[Financial Measures]]]</th>
                <th width="25%">[[[Last Year(Initial Term)]]]</th>
                <th width="25%">[[[Yr1(New Term)]]]</th>
                <th width="25%">[[[Avg Renewal Period]]]</th>
            </tr>
            <tr>
                <td>[[[Anual Rent Expense]]]</td>
                <td>{{fmoutput.AnnualRentExpenseLY|money}}</td>
                <td>{{fmoutput.AnnualRentExpenseYr1|money}}</td>
                <td>{{fmoutput.AnnualRentExpenseAvg|money}}</td>
            </tr>
            <tr>
                <td>[[[Rent as % Prod Sales]]]</td>
                <td>{{fmoutput.RentAsProdSalesLY|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.RentAsProdSalesYr1|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.RentAsProdSalesAvg|numberDisplay:'percent'}}</td>
            </tr>
            <tr>
                <td>[[[Occupancy as % of Prod Sales]]]</td>
                <td>{{fmoutput.OccupancyProdSalesLY|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.OccupancyProdSalesYr1|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.OccupancyProdSalesAvg|numberDisplay:'percent'}}</td>
            </tr>
            <tr>
                <td>[[[SOI as % prod Sales]]]</td>
                <td>{{fmoutput.SOIProdSalesLY|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.SOIProdSalesYr1|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.SOIProdSalesAvg|numberDisplay:'percent'}}</td>
            </tr>
            <tr>
                <td>[[[Cash ROI%]]]</td>
                <td>{{fmoutput.CashROILY|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.CashROIYr1|numberDisplay:'percent'}}</td>
                <td>{{fmoutput.CashROIAvg|numberDisplay:'percent'}}</td>
            </tr>
        </table>
    </div>
</div>

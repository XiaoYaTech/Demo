@{
    Layout = null;
}
<form class="form-horizontal" name="frmApprovalDialog">
    <div class="modal-header">
        <h3 class="modal-title">Renewal GBMemo[[[呈递]]]</h3>
    </div>
    <div class="modal-body">
        <div>
            <div class="form-group">
                <label class="col-xs-3 control-label">[[[Construction Manager]]]</label>
                <div class="col-xs-3">
                    <select class="form-control" chosen="" ng-show="!isLoading" ng-model="Approvers.ConstructionManager" ng-options="m.NameENUS for m in ConstructionManagers">
                        <option value="">[[[-- 请选择 --]]]</option>
                    </select>
                    <img src="~/Content/Images/loading.gif" ng-show="isLoading" />
                    @*<div>{{Approvers.ConstructionManager.Code}}</div>*@
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-sm btn-primary" form-submit ng-click="ok(frmApprovalDialog)">[[[确定]]]</button>
        &nbsp;&nbsp;&nbsp;&nbsp;
        <button class="btn btn-sm btn-warning" ng-click="cancel()">[[[取消]]]</button>
    </div>
</form>

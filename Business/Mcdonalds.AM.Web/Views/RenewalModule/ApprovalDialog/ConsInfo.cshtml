
@{
    Layout = null;
}
<div>
    <div class="modal-header">
        <h3 class="modal-title">Cons Info[[[呈递]]]</h3>
    </div>
    <div class="modal-body">
        <form class="form-horizontal" name="frmApprovalDialog" novalidate>
            <div>
                <div class="form-group">
                    <label class="col-xs-3 control-label">[[[Construction Manager]]]</label>
                    <div class="col-xs-3">
                        <div ng-show="dataLoaded">
                            <select chosen="" class="form-control" name="ConstructionManager" ng-model="Approvers.ConstructionManager" ng-options="m.NameENUS for m in ConstructionManagers" ng-required="true">
                                <option value="">[[[-- 请选择 --]]]</option>
                            </select>
                        </div>
                        <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                        @*<div>{{Approvers.ConstructionManager.Code}}</div>*@
                        <div ng-show="$submited||frmApprovalDialog.ConstructionManager.$dirty">
                            <span class="ng-error" ng-show="frmApprovalDialog.ConstructionManager.$error.required">[[[必须选择]]]</span>
                        </div>
                    </div>
                    <label class="col-xs-3 control-label">[[[MCCL Cons Manager]]]</label>
                    <div class="col-xs-3">
                        <div ng-show="dataLoaded">
                            <select chosen="" class="form-control" name="MCCLConsManager" ng-model="Approvers.MCCLConsManager" ng-options="m.NameENUS for m in MCCLConsManagers" ng-required="true">
                                <option value="">[[[-- 请选择 --]]]</option>
                            </select>
                        </div>
                        <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                        @*<div>{{Approvers.MCCLConsManager.Code}}</div>*@
                        <div ng-show="$submited||frmApprovalDialog.MCCLConsManager.$dirty">
                            <span class="ng-error" ng-show="frmApprovalDialog.MCCLConsManager.$error.required">[[[必须选择]]]</span>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button class="btn btn-sm btn-primary" form-submit ng-click="ok(frmApprovalDialog)">[[[确定]]]</button>
        &nbsp;&nbsp;&nbsp;&nbsp;
        <button class="btn btn-sm btn-warning" ng-click="cancel()">[[[取消]]]</button>
    </div>
</div>

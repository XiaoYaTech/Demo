@{
    Layout = null;
}
<div class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">
            [[[SOX Audit]]]
        </span>
    </div>
    <div class="panel-body form-horizontal">
        <div class="form-group">
            <div class="col-xs-6">
                [[[1.The lease agreement reflects the options of renewal]]]
            </div>
            <div class="col-xs-5">
                <input type="radio" ng-value="true" ng-model="entity.OptionsForRenewal" ng-disabled="!editable" />&nbsp;&nbsp;[[[Yes]]]
                &nbsp;&nbsp;&nbsp;&nbsp;
                <input type="radio" ng-value="false" ng-model="entity.OptionsForRenewal" ng-disabled="!editable" />&nbsp;&nbsp;[[[No]]]
            </div>
        </div>
        <div class="form-group">
            <div class="col-xs-6">
                [[[2.The lease agreement reflects reinstatement requirement]]]
            </div>
            <div class="col-xs-5">
                <input type="radio" ng-value="true" ng-model="entity.ReinstatementRequirement" ng-disabled="!editable" />&nbsp;&nbsp;[[[Yes]]]
                &nbsp;&nbsp;&nbsp;&nbsp;
                <input type="radio" ng-value="false" ng-model="entity.ReinstatementRequirement" ng-disabled="!editable" />&nbsp;&nbsp;[[[No]]]
            </div>
        </div>
        @*<div class="form-group">
            <div class="col-xs-1">comments</div>
            <div class="col-xs-11">
                <textarea rows="3" ng-model="entity.SOXAuditComment" class="form-control" ng-show="editable"></textarea>
                <pre ng-show="!editable">{{entity.SOXAuditComment}}</pre>
            </div>
        </div>*@
    </div>
</div>


@{
    Layout = null;
}
<script type="text/javascript">
    function showWoInfo() {
        $("#divWOInfo")[0].style.display = "";
    }
</script>

<top-nav project-id="{{projectId}}" flow-code="Closure" sub-code="Closure_ExecutiveSummary"  place-holder="8"></top-nav>
<cheking-point project-id="projectId" flow-code="Closure_ExecutiveSummary" refresh="checkPointRefresh"></cheking-point>

<notification-message project-id="projectId"
                      flow-code="Closure"
                      sub-flow-code="Closure_ExecutiveSummary"
                      node-id="entity.Id"
                      uscode="ClosureInfo.USCode"></notification-message>

<store-basic-info project="ClosureInfo" code="ClosureInfo.USCode"></store-basic-info>
<table class="table table-bordered">
    <tr>
        <th>Item</th>
        <th>Uploaded By</th>
        <th>Uploaded Date</th>
        <th>Status</th>
    </tr>
    <tr ng-repeat="att in templateList">
        <td>
            {{att.Name}}
        </td>
        <td>
            {{att.CreatorName}}
        </td>
        <td>{{att.CreateTime}}</td>
        <td>{{att.IsAvailable}}</td>
    </tr>
</table>
<div id="divInfo2" class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">
            Attachment
        </span>         
    </div>
    <table class="table">
        <tr>
            <th>No.</th>
           
            <th>Item</th>
            <th>Uploaded By</th>
            <th>Uploaded Date</th>
        </tr>
        <tr ng-repeat="att in attachementList">
        
            <td>
                <a ng-href="{{att.FileURL}}">{{att.Name}}</a>
            </td>
            <td></td>
            <td>{{att.CreatorName}}
            </td>
            <td>{{att.CreateTime}}</td>
        </tr>
    </table>
</div>
<div class="form-actions ta_c">
    <button type="button" class="btn btn-lg btn-red" ng-click="ApproverSubmit('Submit')" ><span class="fa fa-check"></span>[[[呈递]]]</button>
    <button type="button" class="btn btn-lg btn-orange" ng-click="ApproverSubmit('Return')" ><span class="fa fa-mail-reply"></span>[[[退回]]]</button>
</div>

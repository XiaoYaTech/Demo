@{
    Layout = null;
}
<div class="panel panel-black mg_t_10">
    <div class="panel-heading" ng-show="editable">
        <span class="panel-icon"></span>
        <span class="panel-title">{{panelTitle}}
        </span>
        <div class="panel-tools">
            <a class="btn btn-sm btn-default" href="{{templateUrl}}">
                <span class="fa fa-cloud-download"></span>
                [[[请先下载模板]]]
            </a>

            <span>&nbsp;&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;      
                   <button id="uploadWoCheckList"
                       type="button"
                       class="btn btn-sm btn-default"
                       plupload-simple
                       upload-url="api/ClosureConsInvtChecking/UploadTemplate/{{projectId}}"
                       mm-types="[{title:'Excel Files',extensions:'xlsx'}]"
                       error-msg="[[[数据验证不通过，请检查后重试]]]"
                       upload-finished="uploadFinished">
                       <span class='fa fa-cloud-upload'></span>
                       [[[结果上传]]]
                   </button>
            </span>
        </div>
        <span class="clear"></span>
    </div>
    <div class="panel-heading clearfix" ng-show="!editable">
        <span class="panel-icon"></span>
        <span class="panel-title">{{panelTitle}}
        </span>
    </div>
    <table class="table">
        <tr>
            <th>[[[Item]]]</th>
            <th>[[[Remark]]]</th>
            <th>[[[Uploaded By]]]</th>
            <th>[[[Uploaded Date]]]</th>
            <th>[[[Approval By]]]</th>
            <th>[[[Approval Date]]]</th>
            <th>[[[Status]]]</th>
        </tr>
        <tr ng-show="!ajaxFinished">
            <td colspan="7" class="ta_c">
                <img src="~/Content/Images/loading.gif" />
            </td>
        </tr>
        <tr ng-repeat="att in templateList" ng-show="ajaxFinished">
            <td><a ng-href="{{att.FileURL}}">{{att.Name}}</a></td>
            <td>[[[调整数据部分]]]</td>
            <td>{{att.CreatorNameENUS}}</td>
            <td>{{att.CreateTime  | date:"yyyy-MM-dd"}}</td>
            <td>{{att.CreatorNameENUS}}</td>
            <td>{{att.CreateTime  | date:"yyyy-MM-dd"}}</td>
            <td>{{att.IsAvailable}}</td>
        </tr>
    </table>
</div>


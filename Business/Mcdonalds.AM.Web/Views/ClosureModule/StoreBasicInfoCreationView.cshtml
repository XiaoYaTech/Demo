@{
    Layout=null;
}
<div class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">
            [[[Store Basic Info]]]
        </span>
        <div class="panel-tools">
            <a href="{{store.linkUrl}}" class="btn btn-sm btn-default">[[[更多 >]]]</a>
        </div>
    </div>
    <div class="panel-body form-horizontal" role="form">
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[US Code]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.Code}}
            </div>
            <label class="col-xs-2 control-label">[[[Store Name CN]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.NameZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[Store Name EN]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.NameENUS}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[Region]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.RegionName}}
            </div>
            <label class="col-xs-2 control-label">[[[Market]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.MarketName}}
            </div>
            <label class="col-xs-2 control-label">[[[City]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.CityName}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[Address(CN)]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.AddressZHCN}}
            </div>
            <label class="col-xs-2 control-label">[[[Open Date]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.OpenDate | date:"yyyy-MM-dd"}}
            </div>
            <label class="col-xs-2 control-label">[[[Close Date]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{project.ActualCloseDate | date:"yyyy-MM-dd"}}
            </div>
        </div>
        <div class="form-group">
            <label class="col-xs-2 control-label">[[[Assets Manager]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.StoreMgrName}}
            </div>
            <label class="col-xs-2 control-label">[[[Assets Rep]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.AssetRepName}}
            </div>
            <label class="col-xs-2 control-label">[[[Assets Actor]]]</label>
            <div class="col-xs-2 pd_t_7">
                {{store.RepMgrName}}
            </div>
        </div>
    </div>
</div>


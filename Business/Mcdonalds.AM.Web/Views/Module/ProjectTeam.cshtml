@{
    Layout = null;
}
<form name="frmProjectTeam">
    <div class="panel panel-orange mg_t_10">
        <div class="panel-heading">
            <span class="panel-icon"></span>
            <span class="panel-title">[[[项目成员]]]
            </span>
            <div class="panel-tools" ng-show="showInnerEdit&&canInnerEdit">
                <button class="btn btn-sm" ng-show="!innerEditing" ng-click="edit()">
                    <span class="fa fa-edit"></span>[[[编辑]]]
                </button>
                <button class="btn btn-sm" ng-show="innerEditing" ng-click="save()">
                    <span class="fa fa-save"></span>[[[保存]]]
                </button>
            </div>
        </div>
        <div class="panel-body form-horizontal" role="form">
            <div class="form-group">
                <label class="control-label col-xs-2">[[[资产代表]]]</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="(editable||innerEditing)&&!isRep">
                            <select chosen="" required class="form-control" name="AssetRep" ng-model="team.AssetRep" ng-options="m.UserNameENUS for m in assetReps">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.AssetRep" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)||isRep" class="pd_t_7">
                            {{team.AssetRep.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
                <label class="control-label col-xs-2">[[[资产发起人]]]</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="(editable||innerEditing)&&!isRep">
                            <select chosen="" required class="form-control" name="AssetActor" ng-model="team.AssetActor" ng-options="m.UserNameENUS for m in assetActors">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.AssetActor" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)||isRep" class="pd_t_7">
                            {{team.AssetActor.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
                <label class="control-label col-xs-2">[[[资产经理]]]</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="editable||innerEditing">
                            <select chosen="" required class="form-control" name="AssetMgrs" ng-model="team.AssetMgr" ng-options="m.UserNameENUS for m in assetMgrs">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.AssetMgrs" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)" class="pd_t_7">
                            {{team.AssetMgr.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-xs-2">[[[财务]]]</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="editable||innerEditing">
                            <select chosen="" required class="form-control" name="Finance" ng-model="team.Finance" ng-options="m.UserNameENUS for m in finances">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.Finance" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)" class="pd_t_7">
                            {{team.Finance.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
                <label class="control-label col-xs-2" ng-hide="flowCode=='Reimage'">[[[Legal]]]</label>
                <div class="col-xs-2" ng-if="flowCode!='Reimage'">
                    <div ng-show="dataLoaded">
                        <div ng-show="(editable||innerEditing)">
                            <select chosen="" required class="form-control" name="Legal" ng-model="team.Legal" ng-options="m.UserNameENUS for m in legals">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.Legal" submited="$root.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)" class="pd_t_7">
                            {{team.Legal.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-xs-2">PM</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="(editable||innerEditing)">
                            <select chosen="" required class="form-control" name="PM" ng-model="team.PM" ng-options="m.UserNameENUS for m in pms">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.PM" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)" class="pd_t_7">
                            {{team.PM.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
                <label class="control-label col-xs-2">[[[Construction Manager]]]</label>
                <div class="col-xs-2">
                    <div ng-show="dataLoaded">
                        <div ng-show="(editable||innerEditing)">
                            <select chosen="" required class="form-control" name="CM" ng-model="team.CM" ng-options="m.UserNameENUS for m in cms">
                                <option value="">[[[请选择]]]</option>
                            </select>
                            <err-msgs field="frmProjectTeam.CM" submited="$parent.$submited"></err-msgs>
                        </div>
                        <div ng-show="!(editable||innerEditing)" class="pd_t_7">
                            {{team.CM.UserNameENUS}}
                        </div>
                    </div>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
            </div>
        </div>
    </div>
</form>


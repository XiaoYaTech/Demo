@{
    Layout = null;
}
<div id="divInfo3" class="panel panel-black mg_t_10">
    <div class="panel-heading">
        <span class="panel-icon"></span>
        <span class="panel-title">[[[联系信息]]]
        </span>
    </div>

    <table class="table">
        <tr>
            <th>[[[姓名]]]</th>
            <th style="padding-left: 15px;">[[[职位]]]</th>
            <th style="padding-left: 15px;">[[[电子邮件]]]</th>
            <th style="padding-left: 15px;">[[[座机]]]</th>
            <th style="padding-left: 15px;">[[[手机]]]</th>
        </tr>
        <tr ng-repeat="contact in contactList">

            <td width="16.66%">{{contact.NameENUS}}</td>
            <td width="16.66%" style="padding-left: 15px;">{{contact.PositionENUS}}</td>
            <td width="33.33%" style="padding-left: 15px;">{{contact.Mail}}</td>
            <td style="padding-left: 15px;">{{contact.Phone}}</td>
            <td style="padding-left: 15px;">{{contact.Mobile}}
            </td>
        </tr>
    </table>

</div>

@{
    Layout = null;
}

<div class="modal-header">
    <h3 class="modal-title">Edit Package</h3>
</div>
<div class="modal-body">
    <form class="form-horizontal" role="form">
        <div class="form-group" ng-repeat="node in canEditNodes">
            <label class="control-label col-xs-1">
                <input type="checkbox" ng-model="node.edit" id="{{'cb'+node.Code}}" ng-disabled="!node.canEdit" ng-change="$parent.editChanged(node,$viewValue)" />
            </label>
            <div class="col-xs-10 pd_t_7">
                <label for="{{'cb'+node.Code}}">{{node.NameENUS}}</label> 
            </div>
        </div>
    </form>
</div>
<div class="modal-footer">
    <button class="btn btn-sm btn-red" ng-click="ok()">[[[确定]]]</button>
    <button class="btn btn-sm btn-yellow" ng-click="cancel()">[[[取消]]]</button>
</div>



                            <option value="8">Renewal (no morm)</option>
                            <option value="9">Major Lease Change (no morm)</option>
                    </select>*@

                <div style="width:120px; display: inline-block;position:absolute;" ng-hide="hideNormType">
                    <dict-selector parent-code="NormType"
                                   selected-code="source.NormType"
                                   selected-dict="SelectedNormType"
                                   editable="editable"
                                   dict-filter="normTypeFilter"
                                   default-select="true"></dict-selector>
                </div>

                &nbsp;&nbsp;&nbsp;&nbsp;
                <span ng-show="!isCheckPage" style="margin-left:120px;">|</span>
                &nbsp;&nbsp;&nbsp;&nbsp;
                <a class="btn btn-sm btn-default" id="downLoadTemplate" ng-click="saveBasicInfo()" ng-show="!isCheckPage">
                    <span class=" fa fa-cloud-download">
                    </span>
                    请先下载模板
                </a>
                <a class="btn btn-sm btn-default" id="downLoadTemplate" href="{{downLoadTemplateUrl}}" ng-show="isCheckPage">
                    <span class="fa fa-cloud-download"></span>
                    请先下载模板
                </a>
                &nbsp;&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;&nbsp;
                <button plupload-simple
                        type="button"
                        class="btn btn-sm btn-default"
                        upload-url="{{uploadApi}}"
                        before-upload="beforeUploadFA"
                        upload-finished="uploadFAFinish"
                        error-msg="请上传正确的模板">
                    <span class='fa fa-cloud-upload'></span>
                    结果上传
                </button>
            </div>
            <span class="clear"></span>
        </div>

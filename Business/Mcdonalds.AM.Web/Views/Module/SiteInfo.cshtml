                        <err-msgs field="siteInfo.McdCarParkCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[公用停车位]]]</label>
                <div class="col-xs-2 pd_t_7" id="divPublicCarParkCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <input type="text" valnumber name="PublicCarParkCount" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.PublicCarParkCount" />
                        <err-msgs field="siteInfo.PublicCarParkCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[路边停车位]]]</label>
                <div class="col-xs-2 pd_t_7" id="divRoadCarParkCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <input type="text" valnumber name="RoadCarParkCount" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.RoadCarParkCount" />
                        <err-msgs field="siteInfo.RoadCarParkCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-xs-2 control-label">[[[停车位合计]]]</label>
                <div class="col-xs-2 pd_t_7" id="divCarParkTotal" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        @*<input type="text" valnumber name="CarParkTotal" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.CarParkTotal" />
                            <div ng-show="$parent.$submited||siteInfo.CarParkTotal.$dirty">
                                <span class="ng-error" ng-show="siteInfo.CarParkTotal.$error.required">[[[必须填写]]]</span>
                                <span class="ng-error" ng-show="siteInfo.CarParkTotal.$error.valnumber">[[[请输入数字]]]</span>
                            </div>*@
                        {{source.CarParkTotal}}
                    </div>
                    <div ng-switch-when="IS">
                        @*<input class="form-control" disabled="disabled" />*@
                        {{source.CarParkTotal}}
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[投资组合]]]</label>
                <div class="col-xs-2 pd_t_7" id="divInvestmentPortfolio" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <dict-selector parent-code="InvestmentPortfolio"
                                       selected-code="source.PortfolioTypeName"
                                       selected-dict="InvestmentPortfolioDict"
                                       dict-filter="['FSDT_GS','FSDT_NonGS','Attached DT','Highway']"
                                       editable="editable"></dict-selector>
                    </div>
                    <div ng-switch-when="IS">
                        {{source.PortfolioTypeNameDisplay}}
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[DT型号]]]</label>
                <div class="col-xs-2 pd_t_7" id="divDTTypeName" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <select chosen="{{editable?'{disable_search:true}':'{disable_search:true,disabled:true}'}}" ng-disabled="!editable" ng-show="dataLoaded" class="form-control" ng-model="source.DTTypeName" ng-options="m.NameZHCN as m.NameZHCN for m in DTTypeNames">
                            <option value="">[[[-- 请选择 --]]]</option>
                        </select>
                        <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                    </div>
                    <div ng-switch-when="IS">
                        @*<input class="form-control" disabled="disabled" />*@
                        {{source.DTTypeNameDisplay}}
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-heading">
            <span class="panel-icon"></span>
            <span class="panel-title">[[[楼层及座位]]]</span>
        </div>

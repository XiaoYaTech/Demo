@{
    Layout = null;
}
<form name="siteInfo">
    <div id="divConstruction" class="panel panel-black mg_t_10">
        <div class="panel-heading">
            <span class="panel-icon"></span>
            <span class="panel-title">
                [[[招牌和停车位]]]
            </span>
        </div>
        <div class="panel-body form-horizontal" role="form">
            <div class="form-group">
                <label class="col-xs-2 control-label">[[[是否有立杆招牌]]]</label>
                <div class="col-xs-2 pd_t_7" id="divPoleSign">
                    <label><input type="radio" name="IsPoleSign" ng-disabled="!editable" value="1" ng-model="source.PoleSign" />  Yes</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <label><input type="radio" name="IsPoleSign" ng-disabled="!editable" value="0" ng-model="source.PoleSign" />  No</label>
                </div>
                <label class="col-xs-2 control-label">[[[立杆招牌数量]]]</label>
                <div class="col-xs-2 pd_t_7" id="divPoleSignageCount">
                    <input type="text" valnumber name="PoleSignageCount"
                           ng-required="source.PoleSign==1"
                           class="form-control"
                           ng-disabled="!editablePoleSign"
                           ng-model="source.PoleSignageCount" />
                    <div ng-show="$parent.$submited||siteInfo.PoleSignageCount.$dirty">
                        <span class="ng-error" ng-show="siteInfo.PoleSignageCount.$error.required">[[[必须填写]]]</span>
                        <span class="ng-error" ng-show="siteInfo.PoleSignageCount.$error.valnumber">[[[请输入数字]]]</span>
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[招牌有效性]]]</label>
                <div class="col-xs-2 pd_t_7" id="divDirectionalEffect">
                    <select chosen="{{editable?'{disable_search:true}':'{disable_search:true,disabled:true}'}}" ng-show="dataLoaded" class="form-control" ng-model="source.DirectionalEffect" ng-options="m.NameZHCN as m.NameZHCN for m in DirectionalEffects">
                        <option value="">[[[-- 请选择 --]]]</option>
                    </select>
                    <img src="~/Content/Images/loading.gif" ng-show="!dataLoaded" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-xs-2 control-label">[[[是否有指示性招牌]]]</label>
                <div class="col-xs-2 pd_t_7" id="divSignage">
                    <label><input type="radio" name="IsSignage" ng-disabled="!editable" value="1" ng-model="source.Signage" />  [[[Yes]]]</label>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <label><input type="radio" name="IsSignage" ng-disabled="!editable" value="0" ng-model="source.Signage" />  [[[No]]]</label>
                </div>
                <label class="col-xs-2 control-label">[[[指示性招牌的数量]]]</label>
                <div class="col-xs-2 pd_t_7" id="divSignageCount">
                    <input type="text" valnumber
                           name="SignageCount"
                           ng-required="source.Signage==1"
                           class="form-control"
                           ng-disabled="!editableSignage"
                           ng-model="source.SignageCount" />
                    <div ng-show="$parent.$submited||siteInfo.SignageCount.$dirty">
                        <span class="ng-error" ng-show="siteInfo.SignageCount.$error.required">[[[必须填写]]]</span>
                        <span class="ng-error" ng-show="siteInfo.SignageCount.$error.valnumber">[[[请输入数字]]]</span>
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[展示面长度（米）]]]</label>
                <div class="col-xs-2 pd_t_7" id="divFacadeLenth">
                    <input type="text" valnumber name="FacadeLenth" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.FacadeLenth" />
                    <err-msgs field="siteInfo.FacadeLenth" submited="$parent.$submited"></err-msgs>
                </div>
            </div>
            <div class="form-group">
                <label class="col-xs-2 control-label">[[[出口数量]]]</label>
                <div class="col-xs-2 pd_t_7" id="divEgressCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <input type="text" valnumber name="EgressCount" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.EgressCount" />
                        <err-msgs field="siteInfo.EgressCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[进口数量]]]</label>
                <div class="col-xs-2 pd_t_7" id="divIngressCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <input type="text" valnumber name="IngressCount" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.IngressCount" />
                        <err-msgs field="siteInfo.IngressCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
                <label class="col-xs-2 control-label">[[[开口数量]]]</label>
                <div class="col-xs-2 pd_t_7" id="divOpenCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">
                        <input type="text" valnumber name="OpenCount" ng-required="true" class="form-control" ng-disabled="!editable" ng-model="source.OpenCount" />
                        <err-msgs field="siteInfo.OpenCount" submited="$root.$submited"></err-msgs>
                    </div>
                    <div ng-switch-when="IS">
                        <input class="form-control" disabled="disabled" />
                    </div>
                </div>
            </div>
            <div class="form-group">
                <label class="col-xs-2 control-label">[[[麦当劳停车位]]]</label>
                <div class="col-xs-2 pd_t_7" id="divMcdCarParkCount" ng-switch="store.StoreTypeName">
                    <div ng-switch-when="DT">

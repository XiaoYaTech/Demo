@{
    Layout = null;
}

<div class="modal-header">
    <h3 class="modal-title">选择类型</h3>
</div>
<div class="modal-body">
    <div class="text-center">
        <select multiple="multiple" ng-model="flowType" name="lstUsersPool" size="6" style="width: 230px">
            <option selected="selected" value="Closure">Closure</option>
            <option value="MajorLease">MajorLease</option>
            <option value="Reimage">Reimage</option>
            <option value="Rebuild">Rebuild</option>
            <option value="Renewal">Renewal</option>
        </select>
    </div>
</div>
<div class="modal-footer">
    <button class="btn btn-sm btn-primary" ng-click="ok(flowType[0])">[[[确定]]]</button>
    <button class="btn btn-sm btn-warning" ng-click="cancel()">[[[取消]]]</button>
</div>
